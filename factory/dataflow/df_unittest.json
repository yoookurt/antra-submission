{
	"name": "df_unittest",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_movie_bronze_parquet",
						"type": "DatasetReference"
					},
					"name": "bronze"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "cast1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          movie as string,",
				"          SourceFile as string,",
				"          IngestTime as timestamp,",
				"          p_IngestDate as date,",
				"          Status as string",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> bronze",
				"bronze derive(MovieStruct = movie) ~> derivedColumn1",
				"derivedColumn1 cast(output(",
				"          MovieStruct as ({} as string)",
				"     ),",
				"     errors: true) ~> cast1"
			]
		}
	}
}