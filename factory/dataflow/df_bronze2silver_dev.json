{
	"name": "df_bronze2silver_dev",
	"properties": {
		"folder": {
			"name": "movie"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_movie_bronze_parquet",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "parse1"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          movie as string,",
				"          SourceFile as string,",
				"          IngestTime as timestamp,",
				"          p_IngestDate as date,",
				"          Status as string",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> source1",
				"source1 parse(moviestruct = movie ? (BackdropUrl as string,",
				"          Budget as double,",
				"          CreatedBy as string,",
				"          CreatedDate as string,",
				"          Id as short,",
				"          ImdbUrl as string,",
				"          OriginalLanguage as string,",
				"          Overview as string,",
				"          PosterUrl as string,",
				"          Price as double,",
				"          ReleaseDate as date,",
				"          Revenue as date,",
				"          RunTime as short,",
				"          Tagline as string,",
				"          Title as string,",
				"          TmdbUrl as string,",
				"          UpdatedBy as string,",
				"          UpdatedDate as date,",
				"          genres as (id as short,",
				"          name as string)),",
				"     format: 'json',",
				"     documentForm: 'documentPerLine') ~> parse1",
				"parse1 select(mapColumn(",
				"          MovieId = moviestruct.Id,",
				"          GenreId = moviestruct.genres.id",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1"
			]
		}
	}
}